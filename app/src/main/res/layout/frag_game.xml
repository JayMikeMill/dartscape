                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/tvGamePlayerScore7"
                        style="@style/TextViewPoints"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="5"
                        android:maxLines="1"
                        android:padding="4dp"
                        android:text="WINS" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/lGameAvgsLabel"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight=".35"
                    android:orientation="vertical">

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/tvGamePlayerScore6"
                        style="@style/TextViewPoints"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
