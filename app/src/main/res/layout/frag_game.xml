                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight=".35"
                    android:text="@string/_0"
                    android:textSize="16sp"
                    tools:ignore="NestedWeights" />

            </LinearLayout>

            <LinearLayout
