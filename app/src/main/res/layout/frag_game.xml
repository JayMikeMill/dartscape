                class="com.gworks.dartscape.ui.ScoreButton"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1" />

            <Space
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight=".65" />

            <view
                android:id="@+id/sbGameBaseBallOut"
                class="com.gworks.dartscape.ui.ScoreButton"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                tools:ignore="DuplicateSpeakableTextCheck" />

        </LinearLayout>

        <view
            android:id="@+id/BaseballView"
            class="com.gworks.dartscape.ui.BaseballView"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="?attr/colorAccent"
            android:orientation="horizontal" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:background="?attr/colorButtonNormal"
            android:splitMotionEvents="false">


            <view
                android:id="@+id/btnGameUndo"
                style="@style/MultiViewDefault"
                class="com.gworks.dartscape.ui.MultiView"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@android:color/transparent"
                android:gravity="right"
                android:paddingTop="4dp"
                android:text="@string/undo"
                app:iconColorTint="?attr/titleTextColor"
                app:longPressLoop="true"
                app:longPressLoopDelayMs="150"
                app:textSizeRatio=".7"
                custom:icon1="@drawable/ic_arrow_left" />

            <view
                android:id="@+id/btnGameEndTurn"
                style="@style/MultiViewDefault"
                class="com.gworks.dartscape.ui.MultiView"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@android:color/transparent"
                android:gravity="left"
                android:paddingTop="4dp"
                android:text="@string/end_turn"
                app:iconColorTint="?attr/titleTextColor"
                app:longPressLoop="true"
                app:longPressLoopDelayMs="150"
                custom:icon1="@drawable/ic_arrow_right" />

            <view
                android:id="@+id/btnGameBot"
                style="@style/MultiViewDefault"
                class="com.gworks.dartscape.ui.MultiView"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@android:color/transparent"
                android:gravity="left"
                android:paddingTop="8dp"
                android:text="@string/pause_bot"
                app:iconColorTint="?attr/titleTextColor"
                app:text2="@string/resume_bot"
                custom:icon1="@drawable/ic_pause"
                custom:icon2="@drawable/ic_play" />

            <view
                android:id="@+id/btnGameGameResults"
                style="@style/MultiViewDefault"
                class="com.gworks.dartscape.ui.MultiView"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@android:color/transparent"
                android:gravity="left"
                android:paddingTop="8dp"
                android:text="@string/show_result"
                app:iconColorTint="?attr/titleTextColor"
                custom:icon1="@drawable/ic_plus_sign" />

        </LinearLayout>
    </LinearLayout>
</LinearLayout>

